\newcommand{\tensoro}[1]{
    % \bm{#1}
    {#1}
}

\newcommand{\tensori}[1]{
    % {\stackunder[-1.5pt]{${\bm{#1}}$}{$-$}}
    \bm{#1}
}

\newcommand{\tensorii}[1]{
    {\stackunder[1.2pt]{${\bm{#1}}$}{$\scriptstyle \sim$}}
}

\newcommand{\setclosure}[1]{
    {\stackunder[-10.0pt]{$#1$}{$\scriptstyle --$}}
}

\newcommand{\tensoriis}[1]{
    % \smash{\underset{\sim}{\bm{#1}}}
