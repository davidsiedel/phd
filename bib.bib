	pages = {5383--5425},
	file = {Miehe et al. - 2002 - Anisotropic additive plasticity in the logarithmic.pdf:/home/dsiedel/Documents/literature_all/Miehe et al. - 2002 - Anisotropic additive plasticity in the logarithmic.pdf:application/pdf},
}

