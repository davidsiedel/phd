	pages = {303--329},
	file = {Kabaria et al. - 2015 - A hybridizable discontinuous Galerkin formulation .pdf:/home/dsiedel/Documents/literature_all/Kabaria et al. - 2015 - A hybridizable discontinuous Galerkin formulation .pdf:application/pdf},
