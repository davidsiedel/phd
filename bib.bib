	keywords = {cracks, AS, assumed strain, blending elements, DG, discontinuous Galerkin, dislocations, extended finite element method, interfaces, partition of unity, PU, XFEM},
	pages = {1645--1669},
	file = {Gracie et al. - 2008 - Blending in the extended finite element method by .pdf:/home/dsiedel/Documents/literature_all/Gracie et al. - 2008 - Blending in the extended finite element method by .pdf:application/pdf},
}

@article{shen_optimally_2010,
