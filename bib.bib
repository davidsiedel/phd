	file = {Nguyen et al. - 2011 - An implicit high-order hybridizable discontinuous .pdf:/home/dsiedel/Documents/literature_all/Nguyen et al. - 2011 - An implicit high-order hybridizable discontinuous .pdf:application/pdf},
}

