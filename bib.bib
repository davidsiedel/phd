	author = {Cockburn, Bernardo},
	editor = {Barrenechea, Gabriel R. and Brezzi, Franco and Cangiani, Andrea and Georgoulis, Emmanuil H.},
	year = {2016},
	doi = {10.1007/978-3-319-41640-3_5},
	note = {Series Title: Lecture Notes in Computational Science and Engineering},
	pages = {129--177},
	file = {Cockburn - 2016 - Static Condensation, Hybridization, and the Devisi.pdf:/home/dsiedel/Documents/literature_all/Cockburn - 2016 - Static Condensation, Hybridization, and the Devisi.pdf:application/pdf},
