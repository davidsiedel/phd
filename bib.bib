	author = {Aldakheel, Fadi and Hudobivnik, Blaž and Hussein, Ali and Wriggers, Peter},
	month = nov,
	year = {2018},
	pages = {443--466},
	file = {Aldakheel et al. - 2018 - Phase-field modeling of brittle fracture using an .pdf:/home/dsiedel/Documents/literature_all/Aldakheel et al. - 2018 - Phase-field modeling of brittle fracture using an .pdf:application/pdf},
}

@article{alessi_gradient_2015,
	title = {Gradient damage models coupled with plasticity: {Variational} formulation and main properties},
	volume = {80},
	issn = {01676636},
	shorttitle = {Gradient damage models coupled with plasticity},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0167663614000039},
	doi = {10.1016/j.mechmat.2013.12.005},
	abstract = {A variational formulation is proposed for a family of elastic–plastic–damage models within the framework of rate-independent materials. That consists ﬁrst in deﬁning the total energy which contains, in particular, a gradient damage term and a term which represents the plastic dissipation but depends also on damage. Then, the evolution law is deduced from the principles of irreversibility, stability and energy balance. Accordingly, the plastic dissipation term which appears both in the damage criterion and the plastic yield criterion plays an essential role in the damage–plasticity coupling. Suitable constitutive choices on how the plastic yield stress decreases with damage, allows us to obtain a rich variety of coupled responses. A particular attention is paid on the equations which govern the formation of cohesive cracks where the displacement is discontinuous and the plasticity localizes. In the one-dimensional traction test where the solution is obtained in a closed form, we show that, because of damage localization, a cohesive crack really appears at the center of the damage zone before the rupture and the associated cohesive law is obtained in closed form in terms of the constitutive parameters. A Finite Element discrete version of the energy functional is used to simulate a two-dimensional traction test over a rectangular domain with mixed boundary conditions; again a localized band of plastic strain is generated seemingly independent of the mesh size.},
	language = {en},
	urldate = {2021-11-18},
	journal = {Mechanics of Materials},
	author = {Alessi, Roberto and Marigo, Jean-Jacques and Vidoli, Stefano},
	month = jan,
	year = {2015},
	pages = {351--367},
	file = {Alessi et al. - 2015 - Gradient damage models coupled with plasticity Va.pdf:/home/dsiedel/Documents/literature_all/Alessi et al. - 2015 - Gradient damage models coupled with plasticity Va.pdf:application/pdf},
}

