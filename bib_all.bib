	title = {An implicit high-order hybridizable discontinuous {Galerkin} method for the incompressible {Navier}–{Stokes} equations},
	volume = {230},
	issn = {00219991},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0021999110005887},
