@inproceedings{petry_cyrano3_2015,
	title = {{CYRANO3} fuel performance code: modelling fuel behaviour during in-core operation, transport and storage},
	url = {http://inis.iaea.org/search/search.aspx?orig_q=RN:46081717},
	abstract = {Thanks to an integrated approach, the CYRANO3 fuel performance code allows a complete description of PWR fuels behaviour from the early stages of in-core irradiation to decades of storage To this end, a modular approach is used, including highly coupled multi-physics models (eg neutronics, physics, mechanics), each of them being developed and identified on the basis of dedicated experimental investigations In the present paper, a focus on spent fuel modelling during transport and dry storage is proposed In CYRANO3, a spent fuel rod model is determined thanks to a first in-core computation run; its description thus includes several physical and mechanical parameters that might affect the rods behaviour during transport and storage Then, a second computation run is performed in order to determine the evolution of the rods clad creep strain during specific transport/storage conditions The maximum creep strain value can then be compared to an experimentally determined failure criterion Taking advantage of the broad validation range of CYRANO3 code, computations can be applied to a large variety of in-core and out-of-core scenarios, including design and safety analyses In this paper, the problem of claddings creep excessive deformation during transport or dry storage of spent fuel assemblies is addressed Typical evolutions of the spent fuel rods properties are detailed, such as geometric or mechanical changes, showing the importance of a good knowledge of spent fuel rods inner pressure during their whole life - which relies on a correct prediction of the free volumes and the fission gas release in the whole rod - and irradiated cladding materials creep properties in the different transport/storage expected conditions (temperature and stress ranges) Taking all these aspects into account, it is shown that, even with conservative assumptions regarding temperature levels of the different phases, important margins usually exist regarding clad creep excessive deformation during transport or dry storage of spent fuel assemblies},
	author = {Petry, C. and Bouffioux, P. and Thouvenin, G.},
	year = {2015},
	note = {event-place: France},
}

@inproceedings{largenton_cyrano3_2014,
	title = {{CYRANO3} {The} {EDF} fuel performance code {Global} overview and recent developments on fission gas modelling},
	url = {http://inis.iaea.org/search/search.aspx?orig_q=RN:47070636},
	abstract = {CYRANO3 is the thermal mechanical industrial code developed and used by EDF to simulate the nuclear fuel rod performance in pile, as well as under incidental conditions and during the disposal storage period This code has already been used successfully for the last ten years by the EDF engineering teams in justifying the overall reloading options covering various fuels: UO\_2, UO\_2 + gadolinium, MOX, UO\_2 plus additives, and various cladding proposed by the nuclear fuel suppliers Heterogeneous models are used for MOX fuels in order to account for differential evolutions between phases This is a second generation code built with a one-dimensional thermal and mechanical finite elements kernel An appropriate architecture allows plugging in any new functionalities or models The objective is to provide the EDF designers with a convivial and qualified code, with robust algorithms and able to quickly evolve with the computer performances Algorithms are able to account for adiabatic transients and are robust enough to avoid any undesirable overflows The code is extremely documented and takes benefit of a large data base accumulated since the seventies Even if CYRANO3 is mainly based on a one-dimensional radial approach, simulation is efficiently enriched to account for axial and hoop structural phenomena, using analytical solutions The CYRANO3 is now integrated since more than ten years in the CEA PLEIADES platform This publication deals with a global overview of the capability of the code and focused on recent innovative evolutions concerning the fission gas modeling, by the use of the model named CARACAS, developed in the framework of a CEA/EDF/AREVA project, and recently implemented and validated in the CYRANO3 code (author)},
	author = {Largenton, Rodrigue and Thouvenin, Gilles},
	year = {2014},
	note = {event-place: Japan},
	pages = {784},
}

