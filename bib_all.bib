	author = {Lorentz, E. and Godard, V.},
	month = may,
	year = {2011},
	keywords = {Augmented lagrangian, Damage, Gradient constitutive laws, Strain-softening, Variational formulation},
	pages = {1927--1944},
}

@article{son_standard_2021,
