	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/nme.2781},
	keywords = {fracture, discontinuous Galerkin, extended finite element method, immersed boundary method, lifting operator, solids},
	pages = {716--755},
	file = {Shen and Lew - 2010 - An optimally convergent discontinuous Galerkin-bas.pdf:/home/dsiedel/Documents/literature_all/Shen and Lew - 2010 - An optimally convergent discontinuous Galerkin-bas.pdf:application/pdf},
}

