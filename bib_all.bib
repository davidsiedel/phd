	pages = {2765--2778},
	file = {Miehe et al. - 2010 - A phase field model for rate-independent crack pro.pdf:/home/dsiedel/Documents/literature_all/Miehe et al. - 2010 - A phase field model for rate-independent crack pro.pdf:application/pdf;Miehe et al. - 2010 - A phase field model for rate-independent crack pro.pdf:/home/dsiedel/Zotero/storage/DC7MBME2/Miehe et al. - 2010 - A phase field model for rate-independent crack pro.pdf:application/pdf;Miehe et al. - 2010 - A phase field model for rate-independent crack pro.pdf:/home/dsiedel/Zotero/storage/G7U5SWER/Miehe et al. - 2010 - A phase field model for rate-independent crack pro.pdf:application/pdf},
