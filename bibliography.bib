@article{azinpour_simple_2018,
	title = {A simple and unified implementation of phase field and gradient damage models},
	volume = {5},
	issn = {2213-7467},
	url = {https://doi.org/10.1186/s40323-018-0106-7},
	doi = {10.1186/s40323-018-0106-7},
	abstract = {In this work, the analogous treatment between coupled temperature–displacement problems and material failure models is explored within the context of a commercial software (Abaqus®). The implicit gradient Lemaitre damage and phase field models are implemented utilizing the software underlying capabilities for coupled temperature–displacement problems. The heat conduction equation is made compatible with the diffusive regularization of such material models and calculations are carried out at the material point level. This bypasses the need to implement explicitly the weak form resultant from the coupling between the momentum conservation and the evolution of the diffusive field. Throughout benchmarking examples, the proposed methodology is assessed and validated by investigating typical issues risen from the considered local inelastic-based deformation models, such as mesh dependency and the difficulties to predict cracked regions.},
	pages = {15},
	number = {1},
	journaltitle = {Advanced Modeling and Simulation in Engineering Sciences},
	shortjournal = {Advanced Modeling and Simulation in Engineering Sciences},
	author = {Azinpour, E. and Ferreira, J. P. S. and Parente, M. P. L. and de Sa, J. Cesar},
	urldate = {2022-06-08},
	date = {2018-05-21},
	keywords = {Commercial software, Diffusive regularization, Implicit gradient Lemaitre damage model, Material failure, Mesh dependency, Phase field model},
	file = {Full Text PDF:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/B6844U4L/Azinpour et al. - 2018 - A simple and unified implementation of phase field.pdf:application/pdf;Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/YQKGFZC7/s40323-018-0106-7.html:text/html}
}


@article{alessi_phase-field_2020,
	title = {Phase-field numerical strategies for deviatoric driven fractures},
	volume = {359},
	issn = {0045-7825},
	url = {https://www.sciencedirect.com/science/article/pii/S0045782519305365},
	doi = {10.1016/j.cma.2019.112651},
	abstract = {The phase-field approach regularizes the variational theory of fracture by approximating cracks with a smeared damage field. In this work, the attention is focused on those formulations approximating mode {II} fractures (shear fractures). In these models, only the deviatoric part of the strain elastic energy, penalized by the phase-field, drives the crack onset and evolution, whereas the elastic hydrostatic energetic contribution has no influence on the failure process. Consequently, cracks evolves according to the von Mises–Hencky–Hüber, also known as J2, failure criterion. Unfortunately, volumetric locking problem arises in the damaged zones if classical numerical solution strategies are adopted. As a consequence, damage localization bands appear with an excessive thickness, thus overestimating the fracture energy. In addition, the crack path geometry may be erroneously described because of the loss of precision of the displacement field in damaged zones. To circumvent these drawbacks, two numerical techniques are proposed, namely selective reduced integration and mixed displacement/pressure formulation, and their effectiveness evidenced by a numerical investigation.},
	pages = {112651},
	journaltitle = {Computer Methods in Applied Mechanics and Engineering},
	shortjournal = {Computer Methods in Applied Mechanics and Engineering},
	author = {Alessi, R. and Freddi, F. and Mingazzi, L.},
	urldate = {2022-04-06},
	date = {2020-02-01},
	langid = {english},
	keywords = {Phase-field, Gamma-convergence, Shear fracture, Variational fracture mechanics},
	file = {Alessi et al. - 2020 - Phase-field numerical strategies for deviatoric dr.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/MSI8FT9Z/Alessi et al. - 2020 - Phase-field numerical strategies for deviatoric dr.pdf:application/pdf;ScienceDirect Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/M3XPLZVG/S0045782519305365.html:text/html;ScienceDirect Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/9MB6HBS6/S0045782519305365.html:text/html;ScienceDirect Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/J3KJINPP/S0045782519305365.html:text/html}
}

