@article{gatt_experimental_2015,
  title        = {Experimental and numerical study of fracture mechanisms in {UO}2 nuclear fuel},
  volume       = {47},
  issn         = {1350-6307},
  url          = {http://www.sciencedirect.com/science/article/pii/S1350630714002441},
