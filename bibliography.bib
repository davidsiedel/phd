  keywords     = {Cracks, Phase Field, Nucleation, Microtomography, Voxel models, Heterogeneous materials},
  file         = {Nguyen et al. - 2015 - A phase field method to simulate crack nucleation .pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/S5ZMF3GH/Nguyen et al. - 2015 - A phase field method to simulate crack nucleation .pdf:application/pdf;ScienceDirect Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/EEZUN746/S0013794415001332.html:text/html}
}

@article{nguyen_phase-field_2016,
  title        = {A phase-field method for computational modeling of interfacial damage interacting with crack propagation in realistic microstructures obtained by microtomography},
